{
    "name": "wpeasy/ab-bricks-auto-token",
    "description": "Automatically generates Bricks Builder Dynamic Tokens and Conditions based on ACF/MetaBox field naming conventions",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "WPEasy",
            "homepage": "https://github.com/wpeasy"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "autoload": {
        "psr-4": {
            "AB\\BricksAutoToken\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
